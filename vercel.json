{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/proxy.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/proxy.py"
    }
  ]
}
